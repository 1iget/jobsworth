<% @page_title = t('milestones.edit_title', title: Setting.productName) %>

<legend><%= t('milestones.milestones') %></legend>

<% for milestone in @scheduled_milestones %>
  <%= render :partial => 'milestones/milestone_row', :locals => {:milestone => milestone } %>
<% end %>

<% for milestone in @unscheduled_milestones %>
  <%= render :partial => 'milestones/milestone_row', :locals => {:milestone => milestone } %>
<% end %>