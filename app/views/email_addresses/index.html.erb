<% content_for :navigation do %>
  <%= render :partial => "companies/navigation", :locals => {:selected => "orphaned-emails"} %>
<% end %>

<legend>Orphaned Email Address</legend>

<% @email_addresses.each do |ea| %>
  <div class="well pull-left" style="margin-right: 20px;">
    <%= link_to ea.email, edit_email_address_path(ea) %>
  </div>
<% end %>

<%= will_paginate(@email_addresses, { :next_label => _('Next') + ' &raquo;'.html_safe, :prev_label => '&laquo; '.html_safe + _('Previous') }) %>

