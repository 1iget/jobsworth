<%= @tasks.collect{|task|
  if task.resolved?
    to = tz.utc_to_local(task.updated_at).to_i * 1000
    from = to - task.duration * 60 * 1000
  else
    from = tz.utc_to_local(task.estimate_date || Time.now).to_i * 1000
    to = from + task.duration * 60 * 1000
  end

  {
   :name => link_to("##{task.task_num}", task),
   :desc => task.name,
   :values => [ {
     :from => "/Date(#{from})/",
     :to => "/Date(#{to})/",
     :desc => task.name,
     :label => task.name
   }]
  }
}.to_json.html_safe
-%>
